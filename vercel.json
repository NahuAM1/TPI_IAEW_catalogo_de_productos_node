{
    "version": 2,
    "builds": [
        {
            "src": "dist/commons/integrations/swagger.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/commons/integrations/swagger.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        },
        {
            "src": "/apidocumentation",
            "dest": "/dist/commons/integrations/swagger.js"
        }
    ]
}
